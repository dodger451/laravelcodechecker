{
    "name": "dodger451/laravelcodechecker",
    "description": "Basic configurable phpmd, phpcs and lint setup with artisan support",
    "license": "MIT",
    "authors": [
        {
            "name": "david latotzky",
            "email": "david-laravelcodechecker@latotzky.de",
            "homepage": "http://david.latotzky.de"
        }
    ],
    "homepage": "https://github.com/dodger451/laravelcodechecker",
    "keywords": ["Laravel", "LaravelCodeChecker"],
    "require": {
        "illuminate/support": "~5",
        "symfony/process": "^4.1",
        "squizlabs/php_codesniffer": "^3.2",
        "phpmd/phpmd": "^2.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0",
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "dodger451\\LaravelCodeChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dodger451\\LaravelCodeChecker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "dodger451\\LaravelCodeChecker\\LaravelCodeCheckerServiceProvider"
            ]
        }
    }
}
